<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Cabinet.
    -->
    <changeSet id="20220426214107-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="productfeed_id"
                                 baseTableName="cabinet"
                                 constraintName="fk_cabinet__productfeed_id"
                                 referencedColumnNames="id"
